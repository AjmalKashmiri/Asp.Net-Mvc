@model IEnumerable<CarsManagement.Models.Products>

@foreach (var item in Model)
{
    <div class="col-md-4 panel panel-success">
        <div class="col-md-12 panel-heading">
            <p>@item.ProductName</p>
        </div>
        <div class="panel-body">
            <div class="col-md-12 backcoler">

                <img class="col-md-4 img-responsive backimage" src="~/Data/Images/@item.ProductImage" />
            </div>

        </div>
        <div>
            <a href="AdminPanel/ProductGalleryDetails?Id=@item.Id&CatId=@item.CatId">Details</a>
        </div>
    </div>
}

<div class="clearfix"></div>
