<h1 class="alert-success">
    Welcome to Adminstrater Panel:
    @ViewData["SessionName"] @ViewData["Count"]<span style="float:right;color:darkseagreen">
        @ViewData["SessionType"]
    </span>
</h1>
<div style="border:2px solid">

    @if (ViewBag.owner == ViewBag.type)
    {
        <a href="SignUp"><input type="button" class="btn btn-success" value="Add User" /></a>
    }
    <a href="AddCategry"><input type="button" class="btn btn-info" value="Add Categry" /></a>
    <a href="AddSubCategry"><input type="button" class="btn btn-info" value="Add Sub-categry" /></a>
    <a href="AddProduct"><input type="button" class="btn btn-info" value="Add Product" /></a>
    <a href="LogOut"><input style="float:right;margin:5px 5px 0px 0px " class="btn-danger" type="button" value="Logout" /></a>
</div>

<div style="border:2px solid red;width:100%;height:700px;">

    <div style="border:4px solid green;width:18%;height:700px;">
        <ul style="width:100%;padding:10px 10px 10px 10px;list-style:none;font-size:30px;">
            <li style="background-color:grey;color:white;text-align:center;">Categries</li>
            @model IEnumerable<CarsManagement.Models.Categry>

            @foreach (var item in Model)
            {
                <li style="text-align:left;">
                    <a href="ShowProducts?cat=@Html.DisplayFor(modelItem => item.Id)">
                        @Html.DisplayFor(modelItem => item.CatName)
                    </a>
                </li>


                //withOut Query String
                @*<li style="text-align:left;">
                        <a href="@Url.Action("ShowProducts","AdminPanel",new {cat=@item.Id})">
                            @Html.DisplayFor(modelItem => item.CatName)
                        </a>
                    </li>*@
            }

        </ul>
    </div>

</div>
